{
  "manifest_version": 4,
  "name": "YT Reply Filter",
  "version": "5.0.6",
  "description": "Hides spam comments (eg uttp shit ect)",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["filter.js"],
      "run_at": "document_end"
    }
  ]
}
